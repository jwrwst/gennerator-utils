<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
     <comment>代码生成器配置文件:</comment>
     <!--数据库字段类型与实体字段类型对应关系-->
     <entry key="CHAR">String</entry>
     <entry key="VARCHAR">String</entry>
     <entry key="LONGVARCHAR">String</entry>
     <entry key="TINYINT">Integer</entry>
     <entry key="NUMERIC">Double</entry>
     <entry key="DECIMAL">java.math.BigDecimal</entry>
     <entry key="BIGINT">Long</entry>
     <entry key="REAL">Double</entry>
     <entry key="SMALLINT">Integer</entry>
     <entry key="INTEGER">Integer</entry>
     <entry key="INT">Integer</entry>
     <entry key="DATE">java.util.Date</entry>
     <entry key="TIME">java.util.Date</entry>
     <entry key="DATETIME">java.util.Date</entry>
     <entry key="TIMESTAMP">java.util.Date</entry>
     <entry key="BIT">Boolean</entry>

    <!-- oracle需要指定jdbc.schema,其它数据库忽略此项配置  -->
    <entry key="jdbc.schema"/>
    <entry key="jdbc.catalog"/>

     <!--包名目录-->
     <entry key="basepackage">cn</entry>
     <entry key="outRoot">/GeneratorCode</entry>

     <entry key="jdbc.username">root</entry>
     <entry key="jdbc.password">123456</entry>

     <!-- 需要移除的表名前缀,使用逗号进行分隔多个前缀,示例值: t_,v_ -->
     <entry key="tableRemovePrefixes">glm_</entry>
     <!-- MySQL -->
     <entry key="jdbc.url">jdbc:mysql://192.168.0.137:3306/230_zi_mxwi?useUnicode=true&amp;useSSL=false&amp;serverTimezone=UTC&amp;characterEncoding=UTF-8</entry>
     <entry key="jdbc.driver">com.mysql.cj.jdbc.Driver</entry>
     <!--<entry key="jdbc.driver">com.mysql.jdbc.Driver</entry>-->

</properties>
